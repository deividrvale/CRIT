THEORY ints     ;
LOGIC QF_LIA    ;
SOLVER internal ;

SIGNATURE add, a ;

RULES
    add(x, 0, z) -> a ;
    add(x, s(y), 1) -> a ;
    add(y, x, 3) -> a ;
    add(x, x, y) -> a ;
    add(0, 0, 1) -> a ;
    add(0, 1, 1) -> a ;
    add(1, 1, 1) -> a ;

QUERY equivalence add(x1, y1, y1) -><- a [x1 = y1 = 0]

END OF FILE


